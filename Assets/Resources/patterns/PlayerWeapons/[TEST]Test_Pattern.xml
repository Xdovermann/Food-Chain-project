<?xml version="1.0" ?>
<!DOCTYPE bulletml SYSTEM "bulletml.dtd">
<bulletml>
	<action label="top">
		<repeat>
			<times>4</times>
			<action>
				<fire>
					<bullet label="Enemy_Projectile">
						<direction type="sequence">90+180</direction>
						<speed>0.75</speed>
						<action>
							<wait>5</wait>
							<changeDirection>
								<direction type="aim">0</direction>
								<term>0</term>
							</changeDirection>
						</action>
					</bullet>
				</fire>

			</action>
		</repeat>

	</action>






</bulletml>